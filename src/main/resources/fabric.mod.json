{
  "schemaVersion": 1,
  "id": "biomeinfo",
  "version": "${version}",

  "name": "Biome Info",
  "description": "Adds a text to the top left corner saying what biome the player is in.",
  "authors": [
    "bl4ckscor3"
  ],
  "contact": {
    "homepage": "https://curseforge.com/minecraft/mc-mods/biomeinfo-fabric",
    "issues": "https://github.com/bl4ckscor3/BiomeInfo/issues",
    "sources": "https://github.com/bl4ckscor3/BiomeInfo/tree/1.16-fabric"
  },

  "license": "GPL-3.0",
  "icon": "assets/biomeinfo/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "bl4ckscor3.mod.biomeinfo.BiomeInfo"
    ]
  },

  "depends": {
    "fabricloader": ">=0.7.0"
  },
  "suggests": {
    "flamingo": "*"
  },

  "custom": {
    "modmenu:clientsideOnly": true
  }
}
